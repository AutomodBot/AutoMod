{
  "name": "automod",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "./node_modules/.bin/ts-node-transpile-only -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "start-debug": "DEBUG=true LOG_LEVEL=silly ./bin/automod",
    "test": "ava",
    "coverage": "nyc ava",
    "build": "npm run bundle-minify",
    "bundle": "npx tsup ./src/index.ts",
    "bundle-minify": "npx tsup ./src/index.ts --minify",
    "migrate": "node -r dotenv/config ./node_modules/.bin/pg-migrations apply --directory ./migrations",
    "migrate-reapply": "node -r dotenv/config ./node_modules/.bin/pg-migrations mark-unapplied --directory ./migrations -m"
  },
  "keywords": [],
  "author": "Alexis Tyler <xo@wvvw.me>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0 <15.0.0"
  },
  "dependencies": {
    "@databases/pg": "^3.1.3",
    "axios": "^0.21.0",
    "better-errors": "^0.1.2",
    "canvacord": "file:canvacord-5.0.8.tgz",
    "chalk": "^4.1.0",
    "dedent": "^0.7.0",
    "discord-canvas": "^1.4.1",
    "discord-slash-commands-client": "^1.1.0",
    "discord.js": "git+https://github.com/MatteZ02/discord.js.git",
    "discord.js-menu": "^2.3.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "get-current-line": "^6.3.0",
    "git-rev-sync": "^3.0.1",
    "humanize-duration": "^3.25.0",
    "import-to-array": "^1.0.0",
    "jsdiscordperms": "github:PgBiel/jsdiscordperms",
    "logger": "github:unraid/logger#master",
    "ml-sentiment": "^2.0.7",
    "njstrace": "^1.2.0",
    "number-to-color": "^0.4.1",
    "p-timeout": "^4.0.1",
    "parse-human-date": "^1.2.0",
    "pluralize": "^8.0.0",
    "sentiment": "^5.0.2",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2",
    "uuid-apikey": "^1.5.1",
    "vue": "^2.6.12",
    "vue-server-renderer": "^2.6.12"
  },
  "devDependencies": {
    "@databases/pg-migrations": "^3.1.1",
    "@types/dedent": "^0.7.0",
    "@types/express": "^4.17.9",
    "@types/git-rev-sync": "^2.0.0",
    "@types/humanize-duration": "^3.18.1",
    "@types/node": "^14.14.16",
    "@types/node-fetch": "^2.5.7",
    "@types/pg": "^7.14.7",
    "@types/sinon": "^9.0.10",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "@types/yesql": "^4.1.0",
    "ava": "^3.14.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.2",
    "ts-mock-imports": "^1.3.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsup": "^3.11.0",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "tsconfig-paths/register",
      "dotenv/config",
      "ts-node/register"
    ]
  }
}
