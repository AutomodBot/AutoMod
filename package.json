  "scripts": {
    "start": "./bin/automod",
    "start-debug": "DEBUG=true LOG_LEVEL=silly ./bin/automod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run bundle-minify",
    "bundle": "npx tsup ./src/index.ts",
