{
  "name": "automod",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "./bin/automod",
    "start-debug": "DEBUG=true LOG_LEVEL=silly ./bin/automod",
    "test": "ava",
    "coverage": "nyc ava",
    "build": "npm run build-api && npm run build-frontend",
    "build-api": "npx tsup ./src/index.ts",
    "build-frontend": "cd frontend && npm run build",
    "migrate": "node -r dotenv/config ./node_modules/.bin/pg-migrations apply --directory ./migrations",
    "migrate-reapply": "node -r dotenv/config ./node_modules/.bin/pg-migrations mark-unapplied --directory ./migrations -m"
  },
