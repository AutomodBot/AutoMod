    "dedent": "^0.7.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "get-current-line": "^6.3.0",
    "import-to-array": "^1.0.0",
